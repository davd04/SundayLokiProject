package com.upskill.Java_Assignments;

public class java_assignemnt2_q7 {
	
	public static void main(String[] args){
		int a =25;
		int b = 30;
		int temp;
		temp = a;
		a = b;
		b = temp;
		
		System.out.println("Swap value a: " + a);
		System.out.println("swap value b: " + b);
		
		question9();
	
	}
	
	public static void question9(){
		String x ="Talen";
		String y = "Tech";
		String temp;
		temp = x;
		x = y;
		y = temp;
		
		System.out.println("swap word 1: "+ x);
		System.out.println("swap word 2: "+ y);
	}
	
}
